{
  "name": "zerodep",
  "version": "2.7.2",
  "license": "MIT",
  "private": true,
  "scripts": {
    "----- MISC -----------": "",
    "loc": "loc dir '!(node_modules|coverage|dist)/**/*.+(md|cjs|js|ts|jsx|tsx|yml|yaml|json|css|scss)'",
    "audit:prod": "npm audit --omit=dev",
    "audit:all": "npm audit",
    "----- NX -------------": "",
    "nx:reset": "rm -rf ./node_modules/.cache/nx && nx reset",
    "nx:affected": "npx nx show projects --affected ",
    "graph:all": "nx graph",
    "graph:affected": "nx affected:graph",
    "----- DOCS -----------": "",
    "docs": "npx docsify serve ./docs",
    "----- FORMATTING -----": "",
    "package:fix": "npm-sort && fixpack && nx run-many --target=packageFix --all --parallel 1",
    "format:all": "nx format:write",
    "format:check": "nx format:check",
    "----- LINTING --------": "",
    "lint:affected": "nx affected --target=lint",
    "lint:affected:fix": "nx affected --target=lint--fix",
    "lint:all": "nx run-many --target=lint --all",
    "lint:all:fix": "nx run-many --target=lint --all --fix",
    "----- TEST --------------": "",
    "test:affected": "nx affected --target=test -- --runInBand",
    "test:all": "nx run-many --target=test --all -- --runInBand",
    "coverage:affected": "nx affected --target=test --coverage",
    "coverage:all": "nx run-many --target=test --all --coverage",
    "coverage:clean": "rm -rf coverage",
    "----- BUILD -------------": "",
    "build:clean": "rm -rf dist && rm -rf tmp && find . -name '.esbuild' -type d | xargs rm -rf && find . -name '.serverless' -type d | xargs rm -rf && find . -name '.webpack' -type d | xargs rm -rf && find . -name '.esbuild' -type d | xargs rm -rf && find . -name '.swc' -type d | xargs rm -rf && npm run nx:reset",
    "build:all": "nx run-many --target=build --all --verbose",
    "----- NPM ---------------": "",
    "npm:patch:affected": "nx affected --target=patch",
    "npm:minor:affected": "nx affected --target=minor",
    "npm:major:affected": "nx affected --target=major",
    "----- NX ----------------": "",
    "nxUpdate": "npx nx migrate latest",
    "nxMigrate": "npx nx migrate --run-migrations --verbose --if-exists",
    "----- NUKE --------------": "",
    "nuke:reinstall": "npm run build:clean && rm -rf ./node_modules && rm -f package-lock.json && npm i && npm audit fix && nx reset"
  },
  "devDependencies": {
    "@nx/eslint": "18.0.8",
    "@nx/eslint-plugin": "18.0.8",
    "@nx/jest": "18.0.8",
    "@nx/js": "18.0.8",
    "@nx/rollup": "18.0.8",
    "@nx/workspace": "18.0.8",
    "@rollup/plugin-strip": "3.0.4",
    "@swc/cli": "~0.1.62",
    "@swc/core": "1.3.93",
    "@swc/helpers": "0.5.3",
    "@swc/jest": "0.2.20",
    "@types/jest": "^29.4.0",
    "@types/node": "18.19.22",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "docsify-cli": "4.4.4",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "fixpack": "4.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-environment-node": "29.7.0",
    "node-loc": "1.1.7",
    "npm-sort": "0.0.4",
    "nx": "18.0.8",
    "prettier": "2.8.8",
    "rollup": "3.29.4",
    "rollup-plugin-ts": "3.4.5",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "tslib": "2.6.2",
    "typescript": "5.3.3",
    "uglify-js": "3.17.4",
    "verdaccio": "5.29.2"
  }
}
