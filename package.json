{
  "name": "zerodep",
  "version": "2.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "----- MISC -----------": "",
    "loc": "loc dir '!(node_modules|coverage|dist)/**/*.+(md|cjs|js|ts|jsx|tsx|yml|yaml|json|css|scss)'",
    "audit:prod": "npm audit --omit=dev",
    "audit:all": "npm audit",
    "----- NX -------------": "",
    "nx:reset": "rm -rf ./node_modules/.cache/nx && nx reset",
    "graph:all": "nx graph",
    "graph:affected": "nx affected:graph",
    "----- DOCS -----------": "",
    "docs": "npx docsify serve ./docs",
    "----- FORMATTING -----": "",
    "package:fix": "npm-sort && fixpack && nx run-many --target=packageFix --all --parallel 1",
    "format:all": "nx format:write",
    "format:check": "nx format:check",
    "----- LINTING --------": "",
    "lint:affected": "nx affected --target=lint --parallel 4",
    "lint:all": "nx run-many --target=lint --all --parallel 4",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "----- TEST --------------": "",
    "test:affected": "nx affected --target=test --parallel 4 -- --runInBand",
    "test:all": "nx run-many --target=test --all --parallel 4 -- --runInBand",
    "coverage:affected": "nx affected --target=test --coverage",
    "coverage:all": "nx run-many --target=test --all --coverage",
    "coverage:clean": "rm -rf coverage",
    "----- BUILD -------------": "",
    "build:clean": "rm -rf dist && rm -rf tmp && find . -name '.esbuild' -type d | xargs rm -rf && find . -name '.serverless' -type d | xargs rm -rf && find . -name '.webpack' -type d | xargs rm -rf && find . -name '.esbuild' -type d | xargs rm -rf && find . -name '.swc' -type d | xargs rm -rf && npm run nx:reset",
    "build:all": "nx run-many --target=build --all --verbose",
    "----- NX ----------------": "",
    "nxUpdate": "npx nx migrate latest",
    "nxMigrate": "npx nx migrate --run-migrations --verbose --if-exists",
    "----- NUKE --------------": "",
    "nuke:reinstall": "npm run build:clean && rm -rf ./node_modules && rm -f package-lock.json && npm i && npm audit fix && nx reset"
  },
  "devDependencies": {
    "@nx/eslint-plugin": "16.4.2",
    "@nx/jest": "16.4.2",
    "@nx/js": "16.4.2",
    "@nx/linter": "16.4.2",
    "@nx/rollup": "16.4.2",
    "@nx/workspace": "16.4.2",
    "@rollup/plugin-strip": "3.0.2",
    "@swc/cli": "~0.1.62",
    "@swc/core": "~1.3.51",
    "@swc/helpers": "~0.5.0",
    "@swc/jest": "0.2.20",
    "@types/jest": "^29.4.0",
    "@types/node": "~18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "docsify-cli": "4.4.4",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "fixpack": "4.0.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.3",
    "jest-environment-node": "^29.4.3",
    "node-loc": "1.1.7",
    "npm-sort": "0.0.4",
    "nx": "16.4.2",
    "prettier": "^2.8.8",
    "rollup": "3.26.0",
    "rollup-plugin-ts": "3.2.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "tslib": "^2.6.0",
    "typescript": "~5.1.3",
    "uglify-js": "3.17.4",
    "verdaccio": "^5.0.4"
  }
}
