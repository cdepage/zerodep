{
  "root": "packages/is/is.promise",
  "sourceRoot": "packages/is/is.promise/src",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "rm -f dist/packages/is/is.promise/*",
          "rollup -c packages/is/is.promise/rollup.config.ts",
          "mv dist/packages/is/is.promise/esm.d.ts dist/packages/is/is.promise/index.d.ts",
          "cp packages/is/is.promise/README.md dist/packages/is/is.promise/README.md",
          "cp packages/is/is.promise/package.json dist/packages/is/is.promise/package.json",
          "uglifyjs dist/packages/is/is.promise/esm.js -o dist/packages/is/is.promise/esm.js -c --annotations --comments /@source/",
          "uglifyjs dist/packages/is/is.promise/cjs.js -o dist/packages/is/is.promise/cjs.js -c --annotations --comments /@source/",
          "uglifyjs dist/packages/is/is.promise/umd.js -o dist/packages/is/is.promise/umd.js -c --annotations --comments /@source/",
          "rm dist/packages/is/is.promise/cjs.d.ts",
          "rm dist/packages/is/is.promise/umd.d.ts"
        ],
        "parallel": false
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/is/is.promise/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/is/is.promise"],
      "options": {
        "jestConfig": "packages/is/is.promise/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "packageFix": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm-sort && fixpack",
        "cwd": "packages/is/is.promise"
      }
    },
    "publish": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm publish --access public",
        "cwd": "dist/packages/is/is.promise"
      }
    }
  },
  "tags": []
}
