{
  "root": "packages/is",
  "sourceRoot": "packages/is/src",
  "targets": {
    "originalBuild": {
      "executor": "@nrwl/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/is",
        "main": "packages/is/src/index.ts",
        "tsConfig": "packages/is/tsconfig.lib.json",
        "assets": ["packages/is/*.md"]
      }
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "rm -rf dist/packages/is",
          "rollup -c packages/is/rollup.config.ts",
          "mv dist/packages/is/index.esm.d.ts dist/packages/is/index.d.ts",
          "rm dist/packages/is/index.cjs.d.ts",
          "cp packages/is/*.md dist/packages/is",
          "cp packages/is/package.json dist/packages/is/package.json",
          "uglifyjs dist/packages/is/index.esm.js -o dist/packages/is/index.esm.min.js -c --annotations --comments /@source/",
          "uglifyjs dist/packages/is/index.cjs.js -o dist/packages/is/index.cjs.min.js -c --annotations --comments /@source/",
          "rm dist/packages/is/index.esm.js",
          "rm dist/packages/is/index.cjs.js"
        ],
        "parallel": false
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/is/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/is"],
      "options": {
        "jestConfig": "packages/is/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "packageFix": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm-sort && fixpack",
        "cwd": "packages/is"
      }
    },
    "publish": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm publish --access public",
        "cwd": "dist/packages/is"
      }
    }
  },
  "tags": []
}
