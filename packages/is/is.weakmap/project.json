{
  "root": "packages/is/is.weakmap",
  "sourceRoot": "packages/is/is.weakmap/src",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "rm -f dist/packages/is/is.weakmap/*",
          "rollup -c packages/is/is.weakmap/rollup.config.ts",
          "mv dist/packages/is/is.weakmap/esm.d.ts dist/packages/is/is.weakmap/index.d.ts",
          "cp packages/is/is.weakmap/README.md dist/packages/is/is.weakmap/README.md",
          "cp packages/is/is.weakmap/package.json dist/packages/is/is.weakmap/package.json",
          "uglifyjs dist/packages/is/is.weakmap/esm.js -o dist/packages/is/is.weakmap/esm.js -c --annotations --comments /@source/",
          "uglifyjs dist/packages/is/is.weakmap/cjs.js -o dist/packages/is/is.weakmap/cjs.js -c --annotations --comments /@source/",
          "uglifyjs dist/packages/is/is.weakmap/umd.js -o dist/packages/is/is.weakmap/umd.js -c --annotations --comments /@source/",
          "rm dist/packages/is/is.weakmap/cjs.d.ts",
          "rm dist/packages/is/is.weakmap/umd.d.ts"
        ],
        "parallel": false
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/is/is.weakmap/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/is/is.weakmap"],
      "options": {
        "jestConfig": "packages/is/is.weakmap/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "packageFix": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm-sort && fixpack",
        "cwd": "packages/is/is.weakmap"
      }
    },
    "publish": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm publish --access public",
        "cwd": "dist/packages/is/is.weakmap"
      }
    }
  },
  "tags": []
}
