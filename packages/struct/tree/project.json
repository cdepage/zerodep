{
  "name": "struct-tree",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/struct/tree/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "rm -rf dist/packages/struct/tree/*",
          "rollup -c packages/struct/tree/rollup.config.mjs",
          "mv dist/packages/struct/tree/esm.d.ts dist/packages/struct/tree/index.d.ts",
          "cp packages/struct/tree/README.md dist/packages/struct/tree/README.md",
          "cp packages/struct/tree/package.json dist/packages/struct/tree/package.json",
          "uglifyjs dist/packages/struct/tree/esm.js -o dist/packages/struct/tree/esm.js -c --no-annotations",
          "rm dist/packages/struct/tree/esm.js"
        ],
        "parallel": false
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "cd dist/packages/struct/collection",
          "npm publish --access=public"
        ]
      },
      "dependsOn": ["build"]
    },
    "packageFix": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm-sort && fixpack",
        "cwd": "packages/struct/tree"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/struct/tree/**/*.ts",
          "packages/struct/tree/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/struct/tree/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
