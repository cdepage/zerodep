{
  "name": "struct-stack",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/struct/stack/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "rm -rf dist/packages/struct/stack/*",
          "rollup -c packages/struct/stack/rollup.config.mjs",
          "mv dist/packages/struct/stack/esm.d.mts dist/packages/struct/stack/index.d.ts",
          "cp packages/struct/stack/README.md dist/packages/struct/stack/README.md",
          "cp LICENSE.md dist/packages/struct/stack/LICENSE.md",
          "cp SECURITY.md dist/packages/struct/stack/SECURITY.md",
          "cp packages/struct/stack/package.json dist/packages/struct/stack/package.json",
          "uglifyjs dist/packages/struct/stack/esm.mjs -o dist/packages/struct/stack/esm.mjs -c --no-annotations",
          "uglifyjs dist/packages/struct/stack/cjs.cjs -o dist/packages/struct/stack/cjs.cjs -c --no-annotations",
          "rm -rf dist/packages/struct/stack/cjs.d.cts"
        ],
        "parallel": false
      }
    },
    "publish": {
      "command": "cd dist/packages/struct/stack && npm publish --access public"
    },
    "packageFix": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm-sort && fixpack",
        "cwd": "packages/struct/stack"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/struct/stack/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/struct/stack/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
