{
  "name": "struct-linkedlist",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/struct/linkedlist/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "rm -rf dist/packages/struct/linkedlist/*",
          "rollup -c packages/struct/linkedlist/rollup.config.mjs",
          "mv dist/packages/struct/linkedlist/esm.d.ts dist/packages/struct/linkedlist/index.d.ts",
          "cp packages/struct/linkedlist/README.md dist/packages/struct/linkedlist/README.md",
          "cp packages/struct/linkedlist/package.json dist/packages/struct/linkedlist/package.json",
          "uglifyjs dist/packages/struct/linkedlist/esm.js -o dist/packages/struct/linkedlist/esm.js -c --no-annotations",
          "rm dist/packages/struct/linkedlist/esm.js"
        ],
        "parallel": false
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs struct-linkedlist {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/struct/linkedlist/**/*.ts",
          "packages/struct/linkedlist/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/struct/linkedlist/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
