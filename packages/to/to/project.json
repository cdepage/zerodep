{
  "root": "packages/to/to",
  "sourceRoot": "packages/to/to/src",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "rm dist/packages/to/to/*",
          "rollup -c packages/to/to/rollup.config.ts",
          "mv dist/packages/to/to/esm.d.ts dist/packages/to/to/index.d.ts",
          "cp packages/to/to/README.md dist/packages/to/to/README.md",
          "cp packages/to/to/package.json dist/packages/to/to/package.json",
          "uglifyjs dist/packages/to/to/esm.js -o dist/packages/to/to/esm.js -c --annotations --comments /@source/",
          "uglifyjs dist/packages/to/to/cjs.js -o dist/packages/to/to/cjs.js -c --annotations --comments /@source/",
          "uglifyjs dist/packages/to/to/umd.js -o dist/packages/to/to/umd.js -c --annotations --comments /@source/",
          "rm dist/packages/to/to/cjs.d.ts",
          "rm dist/packages/to/to/umd.d.ts"
        ],
        "parallel": false
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/to/to/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/to/to"],
      "options": {
        "jestConfig": "packages/to/to/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "packageFix": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm-sort && fixpack",
        "cwd": "packages/to/to"
      }
    },
    "publish": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm publish --access public",
        "cwd": "dist/packages/to/to"
      }
    }
  },
  "tags": []
}
