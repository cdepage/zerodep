{
  "root": "packages/to/to.array",
  "sourceRoot": "packages/to/to.array/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/to/to.array",
        "main": "packages/to/to.array/src/index.ts",
        "tsConfig": "packages/to/to.array/tsconfig.lib.json",
        "assets": ["packages/to/to.array/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/to/to.array/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/to/to.array"],
      "options": {
        "jestConfig": "packages/to/to.array/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
