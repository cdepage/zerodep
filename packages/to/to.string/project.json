{
  "root": "packages/to/to.string",
  "sourceRoot": "packages/to/to.string/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/to/to.string",
        "main": "packages/to/to.string/src/index.ts",
        "tsConfig": "packages/to/to.string/tsconfig.lib.json",
        "assets": ["packages/to/to.string/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/to/to.string/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/to/to.string"],
      "options": {
        "jestConfig": "packages/to/to.string/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
