{
  "name": "address-zip",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/address/zip/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "rm -rf dist/packages/address/zip/*",
          "rollup -c packages/address/zip/rollup.config.mjs",
          "mv dist/packages/address/zip/esm.d.mts dist/packages/address/zip/index.d.ts",
          "cp packages/address/zip/README.md dist/packages/address/zip/README.md",
          "cp LICENSE.md dist/packages/address/zip/LICENSE.md",
          "cp SECURITY.md dist/packages/address/zip/SECURITY.md",
          "cp packages/address/zip/package.json dist/packages/address/zip/package.json",
          "uglifyjs dist/packages/address/zip/esm.mjs -o dist/packages/address/zip/esm.mjs -c --no-annotations",
          "uglifyjs dist/packages/address/zip/cjs.cjs -o dist/packages/address/zip/cjs.cjs -c --no-annotations",
          "rm -rf dist/packages/address/zip/cjs.d.cts"
        ],
        "parallel": false
      }
    },
    "publish": {
      "command": "cd dist/packages/address/zip && npm publish --access public"
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/address/zip/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/address/zip/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
