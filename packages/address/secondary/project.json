{
  "name": "address-secondary",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/address/secondary/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "rm -rf dist/packages/address/secondary/*",
          "rollup -c packages/address/secondary/rollup.config.mjs",
          "mv dist/packages/address/secondary/esm.d.ts dist/packages/address/secondary/index.d.ts",
          "cp packages/address/secondary/README.md dist/packages/address/secondary/README.md",
          "cp LICENSE.md dist/packages/address/secondary/LICENSE.md",
          "cp SECURITY.md dist/packages/address/secondary/SECURITY.md",
          "cp packages/address/secondary/package.json dist/packages/address/secondary/package.json",
          "uglifyjs dist/packages/address/secondary/esm.js -o dist/packages/address/secondary/esm.js -c --no-annotations",
          "uglifyjs dist/packages/address/secondary/cjs.js -o dist/packages/address/secondary/cjs.js -c --no-annotations",
          "rm -rf dist/packages/address/secondary/cjs.d.ts"
        ],
        "parallel": false
      }
    },
    "publish": {
      "command": "cd dist/packages/address/secondary && npm publish --access public"
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/address/secondary/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/address/secondary/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
