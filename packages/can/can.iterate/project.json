{
  "root": "packages/can/can.iterate",
  "sourceRoot": "packages/can/can.iterate/src",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "rm -f dist/packages/can/can.iterate/*",
          "rollup -c packages/can/can.iterate/rollup.config.ts",
          "mv dist/packages/can/can.iterate/esm.d.ts dist/packages/can/can.iterate/index.d.ts",
          "cp packages/can/can.iterate/README.md dist/packages/can/can.iterate/README.md",
          "cp packages/can/can.iterate/package.json dist/packages/can/can.iterate/package.json",
          "uglifyjs dist/packages/can/can.iterate/esm.js -o dist/packages/can/can.iterate/esm.js -c --annotations --comments /@source/",
          "uglifyjs dist/packages/can/can.iterate/cjs.js -o dist/packages/can/can.iterate/cjs.js -c --annotations --comments /@source/",
          "uglifyjs dist/packages/can/can.iterate/umd.js -o dist/packages/can/can.iterate/umd.js -c --annotations --comments /@source/",
          "rm dist/packages/can/can.iterate/cjs.d.ts",
          "rm dist/packages/can/can.iterate/umd.d.ts"
        ],
        "parallel": false
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/can/can.iterate/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/can/can.iterate"],
      "options": {
        "jestConfig": "packages/can/can.iterate/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "packageFix": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm-sort && fixpack",
        "cwd": "packages/can/can.iterate"
      }
    },
    "publish": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm publish --access public",
        "cwd": "dist/packages/can/can.iterate"
      }
    }
  },
  "tags": []
}
