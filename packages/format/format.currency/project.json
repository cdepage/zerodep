{
  "root": "packages/format/format.currency",
  "sourceRoot": "packages/format/format.currency/src",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "rm -f dist/packages/format/format.currency/*",
          "rollup -c packages/format/format.currency/rollup.config.ts",
          "mv dist/packages/format/format.currency/esm.d.ts dist/packages/format/format.currency/index.d.ts",
          "cp packages/format/format.currency/README.md dist/packages/format/format.currency/README.md",
          "cp packages/format/format.currency/package.json dist/packages/format/format.currency/package.json",
          "uglifyjs dist/packages/format/format.currency/esm.js -o dist/packages/format/format.currency/esm.js -c --annotations --comments /@source/",
          "uglifyjs dist/packages/format/format.currency/cjs.js -o dist/packages/format/format.currency/cjs.js -c --annotations --comments /@source/",
          "uglifyjs dist/packages/format/format.currency/umd.js -o dist/packages/format/format.currency/umd.js -c --annotations --comments /@source/",
          "rm dist/packages/format/format.currency/cjs.d.ts",
          "rm dist/packages/format/format.currency/umd.d.ts"
        ],
        "parallel": false
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/format/format.currency/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/format/format.currency"],
      "options": {
        "jestConfig": "packages/format/format.currency/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "packageFix": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm-sort && fixpack",
        "cwd": "packages/format/format.currency"
      }
    },
    "publish": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm publish --access public",
        "cwd": "dist/packages/format/format.currency"
      }
    }
  },
  "tags": []
}
