{
  "name": "geo-data",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/geo/data/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "rm -rf dist/packages/geo/data/*",
          "rollup -c packages/geo/data/rollup.config.mjs",
          "mv dist/packages/geo/data/esm.d.ts dist/packages/geo/data/index.d.ts",
          "cp packages/geo/data/README.md dist/packages/geo/data/README.md",
          "cp LICENSE.md dist/packages/geo/data/LICENSE.md",
          "cp SECURITY.md dist/packages/geo/data/SECURITY.md",
          "cp packages/geo/data/package.json dist/packages/geo/data/package.json",
          "uglifyjs dist/packages/geo/data/esm.js -o dist/packages/geo/data/esm.js -c --no-annotations",
          "uglifyjs dist/packages/geo/data/cjs.js -o dist/packages/geo/data/cjs.js -c --no-annotations",
          "rm -rf dist/packages/geo/data/cjs.d.ts"
        ],
        "parallel": false
      }
    },
    "publish": {
      "command": "cd dist/packages/geo/data && npm publish --access public"
    },
    "packageFix": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm-sort && fixpack",
        "cwd": "packages/geo/data"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/geo/data/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/geo/data/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
