      "@zerodep/guards.date": ["packages/guards.date/src/index.ts"],
      "@zerodep/guards.errors": ["packages/guards.errors/src/index.ts"],
      "@zerodep/guards.float": ["packages/guards.float/src/index.ts"],
      "@zerodep/guards.integer": ["packages/guards.integer/src/index.ts"],
      "@zerodep/guards.number": ["packages/guards.number/src/index.ts"],
      "@zerodep/guards.object": ["packages/guards.object/src/index.ts"],
      "@zerodep/guards.string": ["packages/guards.string/src/index.ts"],
      "@zerodep/is": ["packages/is/src/index.ts"],
      "@zerodep/is.array": ["packages/is.array/src/index.ts"],
      "@zerodep/is.bigint": ["packages/is.bigint/src/index.ts"],
      "@zerodep/is.boolean": ["packages/is.boolean/src/index.ts"],
      "@zerodep/is.date": ["packages/is.date/src/index.ts"],
      "@zerodep/is.error": ["packages/is.error/src/index.ts"],
      "@zerodep/is.float": ["packages/is.float/src/index.ts"],
      "@zerodep/is.function": ["packages/is.function/src/index.ts"],
      "@zerodep/is.integer": ["packages/is.integer/src/index.ts"],
      "@zerodep/is.map": ["packages/is.map/src/index.ts"],
      "@zerodep/is.null": ["packages/is.null/src/index.ts"],
      "@zerodep/is.number": ["packages/is.number/src/index.ts"],
      "@zerodep/is.object": ["packages/is.object/src/index.ts"],
      "@zerodep/is.regex": ["packages/is.regex/src/index.ts"],
      "@zerodep/is.set": ["packages/is.set/src/index.ts"],
      "@zerodep/is.string": ["packages/is.string/src/index.ts"],
      "@zerodep/is.symbol": ["packages/is.symbol/src/index.ts"],
